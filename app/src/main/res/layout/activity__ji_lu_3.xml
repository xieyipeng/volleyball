<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com2x3b4p.example.volleyball.Activity_JiLu_3">
    <ImageView
        android:src="@drawable/jilu3_back"
        android:scaleType="centerCrop"
        android:id="@+id/jilu3_back"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_weight="3"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <LinearLayout

                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_weight="4"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <Button
                            android:id="@+id/button_choose_1"
                            android:background="@drawable/tubiao_wode_press"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="7"/>
                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/text_duiyuanhao_jilu_1"
                            android:text="1"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1.4"/>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <Button
                            android:id="@+id/button_choose_2"
                            android:background="@drawable/tubiao_wode"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="7"/>
                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/text_duiyuanhao_jilu_2"
                            android:text="2"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1.4"/>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <Button
                            android:id="@+id/button_choose_3"
                            android:background="@drawable/tubiao_wode"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="7"/>
                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/text_duiyuanhao_jilu_3"
                            android:text="3"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1.4"/>
                    </LinearLayout>
                    <View
                        android:background="#cee002"
                        android:layout_width="1dp"
                        android:layout_height="match_parent"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginBottom="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <Button
                            android:id="@+id/button_choose_4"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="7"
                            android:background="@drawable/tubiao_wode" />

                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/text_duiyuanhao_jilu_4"
                            android:text="4"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1.4"/>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <Button
                            android:id="@+id/button_choose_5"
                            android:background="@drawable/tubiao_wode"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="7"/>
                        <TextView
                            android:textColor="#cee002"

                            android:id="@+id/text_duiyuanhao_jilu_5"
                            android:text="5"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1.4"/>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <Button
                            android:id="@+id/button_choose_6"
                            android:background="@drawable/tubiao_wode"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="7"/>
                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/text_duiyuanhao_jilu_6"
                            android:text="6"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1.4"/>
                    </LinearLayout>
                    <View
                        android:background="#cee002"
                        android:layout_width="1dp"
                        android:layout_height="match_parent"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_weight="3"
                    android:layout_height="0dp">
                    <TextView
                        android:textColor="#cee002"
                        android:text="场  序"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_weight="2"
                        android:layout_height="0dp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#cee002"/>
                    <TextView
                        android:id="@+id/text_view_changxu"
                        android:textColor="#cee002"
                        android:textSize="60sp"
                        android:gravity="center"
                        android:text="1"
                        android:layout_width="match_parent"
                        android:layout_weight="4"
                        android:layout_height="0dp" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1">
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#cee002"/>
                    <TextView
                        android:textColor="#cee002"
                        android:gravity="center"
                        android:text="撤  回"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <Button
                        android:layout_gravity="center_horizontal"
                        android:id="@+id/button_chehui"
                        android:layout_weight="1"
                        android:layout_width="40dp"
                        android:layout_height="0dp"
                        android:background="@drawable/button_state_select_chehui" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4">
            <View
                android:background="#cee002"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent">
                        <Button
                            android:id="@+id/button_faqiu"
                            android:layout_margin="3dp"
                            android:background="@drawable/button_color_select"
                            android:text="发   球"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>
                        <Button
                            android:id="@+id/button_yichuan"
                            android:layout_margin="3dp"
                            android:background="@drawable/button_color_select"
                            android:text="一   传"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>
                        <Button
                            android:id="@+id/button_kouqiu"
                            android:layout_margin="3dp"
                            android:background="@drawable/button_color_select"
                            android:text="扣   球"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>
                        <Button
                            android:id="@+id/button_lanwang"
                            android:layout_margin="3dp"
                            android:background="@drawable/button_color_select"
                            android:text="拦   网"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>
                        <Button
                            android:id="@+id/button_fangshou"
                            android:layout_margin="3dp"
                            android:background="@drawable/button_color_select"
                            android:text="防   守"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>
                    </LinearLayout>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#cee002"/>

                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1">

                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/textView4"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="得  分"
                            android:textSize="25sp" />

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <Button
                                android:id="@+id/button_defen_jia"
                                android:layout_centerInParent="true"
                                android:background="@drawable/button_state_select_add"
                                android:layout_width="33dp"
                                android:layout_height="33dp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <Button
                                android:id="@+id/button_defen_jian"
                                android:layout_centerInParent="true"
                                android:background="@drawable/button_state_select_mine"
                                android:layout_width="33dp"
                                android:layout_height="33dp" />
                        </RelativeLayout>
                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/jilu3_defen"
                            android:text="0"
                            android:textSize="30sp"
                            android:layout_width="0dp"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:layout_height="match_parent" />
                    </LinearLayout>
                    <View
                        android:background="#cee002"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1">

                        <TextView
                            android:textColor="#cee002"
                            android:textSize="25sp"
                            android:gravity="center"
                            android:text="一  般"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <Button
                                android:id="@+id/button_yiban_jia"
                                android:layout_centerInParent="true"
                                android:background="@drawable/button_state_select_add"
                                android:layout_width="33dp"
                                android:layout_height="33dp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <Button
                                android:id="@+id/button_yiban_jian"
                                android:layout_centerInParent="true"
                                android:background="@drawable/button_state_select_mine"
                                android:layout_width="33dp"
                                android:layout_height="33dp" />
                        </RelativeLayout>
                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/jilu3_yiban"
                            android:text="0"
                            android:textSize="30sp"
                            android:layout_width="0dp"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:layout_height="match_parent" />
                    </LinearLayout>
                    <View
                        android:background="#cee002"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1">

                        <TextView
                            android:textColor="#cee002"
                            android:textSize="25sp"
                            android:gravity="center"
                            android:text="失  误"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <Button
                                android:id="@+id/button_shiwu_jia"
                                android:layout_centerInParent="true"
                                android:background="@drawable/button_state_select_add"
                                android:layout_width="33dp"
                                android:layout_height="33dp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <Button
                                android:id="@+id/button_shiwu_jian"
                                android:layout_centerInParent="true"
                                android:background="@drawable/button_state_select_mine"
                                android:layout_width="33dp"
                                android:layout_height="33dp" />
                        </RelativeLayout>
                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/jilu3_shiwu"
                            android:text="0"
                            android:textSize="30sp"
                            android:layout_width="0dp"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:layout_height="match_parent" />
                    </LinearLayout>
                    <View
                        android:background="#cee002"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1">

                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/textView5"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="总  数"
                            android:textSize="25sp" />

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <Button
                                android:id="@+id/button_zongshu_jia"
                                android:layout_centerInParent="true"
                                android:background="@drawable/button_state_select_add"
                                android:layout_width="33dp"
                                android:layout_height="33dp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <Button
                                android:id="@+id/button_zongshu_jian"
                                android:layout_centerInParent="true"
                                android:background="@drawable/button_state_select_mine"
                                android:layout_width="33dp"
                                android:layout_height="33dp" />
                        </RelativeLayout>
                        <TextView
                            android:textColor="#cee002"
                            android:id="@+id/jilu3_zongshu"
                            android:text="0"
                            android:textSize="30sp"
                            android:layout_width="0dp"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:layout_height="match_parent" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <View
                android:background="#cee002"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.3"
            >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_weight="3"
                    android:layout_height="match_parent">
                    <TextView
                        android:textColor="#cee002"
                        android:layout_margin="3dp"
                        android:text="小局比分"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="0dp">
                        <EditText
                            android:id="@+id/textView_xiaojubifen_1"
                            android:theme="@style/MyEditText"
                            android:textColor="#cee002"
                            android:gravity="center"
                            android:hint="00"
                            android:textSize="25sp"
                            android:layout_weight="3"
                            android:layout_width="0dp"
                            android:layout_marginLeft="3dp"
                            android:layout_marginRight="3dp"
                            android:layout_height="match_parent" />
                        <TextView
                            android:textSize="30sp"
                            android:text=":"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="match_parent" />
                        <EditText
                            android:id="@+id/textView_xiaojubifen_2"
                            android:theme="@style/MyEditText"
                            android:textColor="#cee002"
                            android:gravity="center"
                            android:hint="00"
                            android:textSize="25sp"
                            android:layout_marginLeft="3dp"
                            android:layout_marginRight="3dp"
                            android:layout_weight="3"
                            android:layout_width="0dp"
                            android:layout_height="match_parent" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4">
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#cee002"/>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:layout_height="match_parent">
                    <Button
                        android:id="@+id/button_data_all"
                        android:layout_margin="2dp"
                        android:background="@drawable/button_color_select"
                        android:textSize="10sp"
                        android:text="数\n据\n统\n计"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        />
                    <Button
                        android:id="@+id/button_shangyiju"
                        android:layout_margin="2dp"
                        android:background="@drawable/button_color_select"
                        android:textSize="10sp"
                        android:text="上\n一\n局"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"/>
                    <Button
                        android:id="@+id/button_xiayiju"
                        android:layout_margin="2dp"
                        android:background="@drawable/button_color_select"
                        android:textSize="10sp"
                        android:text="下\n一\n局"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        />
                    <Button
                        android:id="@+id/button_tijiao"
                        android:layout_margin="2dp"
                        android:background="@drawable/button_color_select"
                        android:textSize="10sp"
                        android:text="提\n\n交"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</FrameLayout>
